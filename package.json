{
  "name": "kafe-web",
  "version": "1.0.0",
  "description": "<h5>Based on ASP.NET Core MVC</h5> <p>Run With Command: dotnet watch run</p>",
  "main": "index.js",
  "scripts": {
    "update-model": "rm KafeWeb.db Migrations/* && dotnet ef migrations add InitialCreate && dotnet ef database update",
    "generate-controller": "dotnet aspnet-codegenerator controller -name <NameController> -m <Model> -dc KafeContext --relativeFolderPath Controllers --useDefaultLayout --referenceScriptLibraries",
    "start": "dotnet watch run"
  },
